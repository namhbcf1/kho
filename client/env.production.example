# Production Environment Configuration
# Copy this file to .env.production and update the values

# API Configuration
REACT_APP_API_URL=https://pos-api.your-domain.com/api
REACT_APP_MOCK_MODE=false
REACT_APP_ENVIRONMENT=production

# Application Configuration
REACT_APP_VERSION=1.0.0
REACT_APP_DEPLOYMENT_URL=https://pos-system.your-domain.com
REACT_APP_COMPANY_NAME=Your Company Name
REACT_APP_SUPPORT_EMAIL=support@your-domain.com

# Error Reporting & Analytics
REACT_APP_SENTRY_DSN=your-sentry-dsn-here
REACT_APP_ANALYTICS_ID=your-analytics-id-here
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_REPORTING=true
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true

# Performance Configuration
REACT_APP_CACHE_DURATION=300000
REACT_APP_MAX_RETRIES=3
REACT_APP_TIMEOUT=10000
REACT_APP_ENABLE_COMPRESSION=true
REACT_APP_ENABLE_MINIFICATION=true

# Security Configuration
REACT_APP_HTTPS_ONLY=true
REACT_APP_SECURE_COOKIES=true
REACT_APP_SESSION_TIMEOUT=30m
REACT_APP_IDLE_TIMEOUT=15m
REACT_APP_MAX_LOGIN_ATTEMPTS=5

# Feature Flags
REACT_APP_FEATURE_FLAGS=ai_features,gamification,commerce_integration
REACT_APP_ENABLE_PWA=true
REACT_APP_OFFLINE_MODE=true
REACT_APP_DARK_MODE_ENABLED=true

# Cloudflare Configuration
REACT_APP_CLOUDFLARE_SITE_ID=your-cloudflare-site-id

# Debug Configuration
REACT_APP_DEBUG_MODE=false
REACT_APP_LOG_LEVEL=error
REACT_APP_SOURCE_MAPS=false

# Maintenance Mode
REACT_APP_MAINTENANCE_MODE=false
REACT_APP_MAINTENANCE_MESSAGE=System is under maintenance. Please try again later.

# Localization
REACT_APP_DEFAULT_LANGUAGE=vi
REACT_APP_SUPPORTED_LANGUAGES=vi,en
REACT_APP_TIMEZONE=Asia/Ho_Chi_Minh
REACT_APP_CURRENCY=VND

# Hardware Integration
REACT_APP_BARCODE_SCANNER_ENABLED=true
REACT_APP_RECEIPT_PRINTER_ENABLED=true
REACT_APP_CASH_DRAWER_ENABLED=true
REACT_APP_PAYMENT_TERMINAL_ENABLED=true

# Business Features
REACT_APP_INVENTORY_TRACKING=true
REACT_APP_SERIAL_NUMBER_TRACKING=true
REACT_APP_FINANCIAL_REPORTING=true
REACT_APP_ROLE_BASED_ACCESS=true
REACT_APP_AUDIT_LOGGING=true 