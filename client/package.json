{
  "name": "pos-frontend",
  "version": "1.0.0",
  "description": "Frontend cho hệ thống POS với Cloudflare Pages",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@hookform/resolvers": "^3.3.2",
    "@reduxjs/toolkit": "^2.8.2",
    "@tanstack/react-query": "^5.8.4",
    "antd": "^5.12.8",
    "axios": "^1.6.2",
    "class-variance-authority": "^0.7.0",
    "cmdk": "^1.1.1",
    "dayjs": "^1.11.10",
    "framer-motion": "^10.18.0",
    "fuse.js": "^7.1.0",
    "html5-qrcode": "^2.3.8",
    "idb": "^8.0.3",
    "lucide-react": "^0.294.0",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.20.1",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.3",
    "recharts": "^2.8.0",
    "redux": "^5.0.1",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.6",
    "tailwindcss": "^3.3.6",
    "web-vitals": "^2.1.4",
    "workbox-core": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0",
    "zod": "^3.22.4",
    "zustand": "^4.5.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:smart": "cypress run --spec 'cypress/e2e/smart-ai-powered-test-2025.cy.js'",
    "cypress:ai": "cypress run --spec 'cypress/e2e/ai-machine-learning-test.cy.js'",
    "cypress:all": "cypress run --spec 'cypress/e2e/*.cy.js'",
    "test:smart": "npm run cypress:smart",
    "test:ai": "npm run cypress:ai",
    "test:comprehensive": "npm run cypress:all",
    "deploy:build": "npm run build && npm run test:comprehensive",
    "deploy:production": "npm run deploy:build && npm run deploy:cloudflare",
    "deploy:cloudflare": "npx wrangler pages deploy build --project-name pos-system-production",
    "health:check": "curl -f https://pos-system-production.pages.dev/health || exit 1",
    "performance:audit": "npx lighthouse https://pos-system-production.pages.dev --output json --output html",
    "security:scan": "npm audit --audit-level moderate",
    "ai:analyze": "echo 'Running AI analysis...' && npm run test:ai",
    "smart:deploy": "npm run security:scan && npm run deploy:production && npm run health:check"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/grep": "^4.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-redux": "^7.1.34",
    "@types/redux": "^3.6.31",
    "@types/socket.io-client": "^1.4.36",
    "cypress": "^14.5.1",
    "cypress-axe": "^1.6.0",
    "cypress-mochawesome-reporter": "^3.8.2",
    "cypress-real-events": "^1.14.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^30.0.4",
    "prettier": "^3.1.0",
    "ts-jest": "^29.4.0",
    "typescript": "^4.9.5",
    "wrangler": "^3.22.0"
  },
  "proxy": "http://localhost:3001",
  "homepage": "https://pos-frontend.pages.dev"
}
