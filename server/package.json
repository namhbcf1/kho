{
  "name": "pos-backend",
  "version": "1.0.0",
  "description": "POS System Backend API",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "wrangler dev",
    "build": "wrangler build",
    "deploy": "wrangler deploy",
    "deploy:staging": "wrangler deploy --env staging",
    "test": "jest",
    "lint": "eslint src routes models --ext .js",
    "lint:fix": "eslint src routes models --ext .js --fix",
    "format": "prettier --write \"src/**/*.js\" \"routes/**/*.js\" \"models/**/*.js\"",
    "migrate": "wrangler d1 migrations apply pos-database",
    "migrate:staging": "wrangler d1 migrations apply pos-database --env staging",
    "seed": "wrangler d1 execute pos-database --file=./migrations/seed.sql",
    "seed:staging": "wrangler d1 execute pos-database --file=./migrations/seed.sql --env staging",
    "db:reset": "wrangler d1 execute pos-database --file=./migrations/reset.sql",
    "db:reset:staging": "wrangler d1 execute pos-database --file=./migrations/reset.sql --env staging"
  },
  "keywords": [
    "pos",
    "api",
    "cloudflare",
    "workers",
    "d1"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^17.0.1",
    "drizzle-orm": "^0.29.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "idb": "^8.0.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "qrcode": "^1.5.4",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "sequelize": "^6.37.7",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1",
    "workbox-core": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0",
    "zod": "^3.22.4",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231218.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.10",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19.1.8",
    "@types/socket.io": "^3.0.1",
    "cypress": "^14.5.1",
    "drizzle-kit": "^0.20.6",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3",
    "wrangler": "^3.22.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
